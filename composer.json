{
    "name": "devgroup/yii2-entity-search-properties",
    "description": "Addon for yii2-entity-search: adds support for properties of yii2-data-structure-tools (dotplant3 properties)",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Alexander Kozhevnikov",
            "email": "b37hr3z3n@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "devgroup/yii2-entity-search": "*",
        "devgroup/yii2-data-structure-tools": "*"
    },
    "require-dev": {
        "codeception/base": "^2.2",
        "codeception/verify": "^0.3.2",
        "codeception/specify": "^0.4.5"
    },
    "autoload": {
        "psr-4": {
            "DevGroup\\EntitySearchProperties\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevGroup\\EntitySearchProperties\\Tests\\": "tests/"
        }
    }
}
